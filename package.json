{
  "name": "simplegameengine",
  "version": "1.0.0",
  "description": "simple pixi game engine",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-old": "tsc ./src-build/webpack-dev.ts && webpack --watch --progress --config ./src-build/webpack-dev.js",
    
        "start": "tsc ./src-build/webpack-dev.ts && webpack-dev-server --open --config ./src-build/webpack-dev.js",
    "start-raid": "tsc ./src-build/run-game-raid.ts && webpack-dev-server --open --config ./src-build/run-game-raid.js",
    "start-pacrpg": "tsc ./src-build/run-game-pacrpg.ts && webpack-dev-server --open --config ./src-build/run-game-pacrpg.js",

    "dev": "tsc ./src-build/webpack-dev.ts && webpack-dev-server --open --config ./src-build/webpack-dev.js",
    "start-server-ld41": "ts-node-dev --respawn --project tsconfig.server.json src/ludumDare41/serverOnly/runServer.ts",
    "build-server-ld41": "tsc --project tsconfig.server.json",
    "start-prod-server-ld41": "node ./lib/serverOnly/runServer.js",
    "build-prod-ld41": "tsc ./src-build/webpack-prod.ld41.ts && webpack --config ./src-build/webpack-prod.ld41.js",
    "build-ld41": "npm run build-server-ld41 && npm run build-prod-ld41",
    "build-dev": "tsc ./src-build/webpack-dev.ts && webpack --config ./src-build/webpack-dev.js",
    "build-prod": "tsc ./src-build/webpack-prod.ts && webpack --config ./src-build/webpack-prod.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/lodash": "^4.14.117",
    "@types/node": "^9.6.6",
    "@types/pixi.js": "^4.8.2",
    "@types/socket.io": "^1.4.33",
    "clean-webpack-plugin": "^0.1.19",
    "freezer-js": "^0.14.1",
    "gamepad-micro": "^0.2.1",
    "hard-source-webpack-plugin": "^0.12.0",
    "howler": "^2.0.15",
    "html-webpack-plugin": "^3.2.0",
    "lockr": "^0.8.5",
    "lodash": "^4.17.11",
    "node-object-hash": "^1.4.1",
    "numeral": "^2.0.6",
    "pixi-keyboard": "^0.9.4",
    "pixi.js": "^4.8.2",
    "prettier": "^1.15.2",
    "socket.io": "^2.1.0",
    "socket.io-client": "^2.1.0",
    "ts-loader": "^5.2.2",
    "tslib": "^1.9.3",
    "typescript": "^3.1.3",
    "victor": "^1.1.0",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
